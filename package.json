        "@types/prismjs": "1.16.3",
        "@types/uuid": "8.3.0",
        "@types/webpack-env": "1.16.0",
        "bootstrap": "4.6.0",
        "chokidar": "3.5.1",
        "codelyzer": "6.0.1",
        "concurrently": "6.0.0",
        "cross-env": "7.0.3",
        "cypress": "6.6.0",
        "cypress-multi-reporters": "1.4.0",
        "dotenv": "8.2.0",
